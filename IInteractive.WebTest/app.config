<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <sectionGroup name="browserConfigGroup">
      <section
        name="browserConfig"
        type="IInteractive.WebTest.BrowserConfigSection, IInteractive.WebTest"
        allowLocation="true"
        allowDefinition="Everywhere"
      />
    </sectionGroup>
  </configSections>
  <browserConfigGroup>
    <browserConfig name="default" 
                   maximumAutomaticRedirections="2" 
                   allowAutoRedirect="true" 
                   userAgent="Mozilla/5.0 (Windows NT 6.1; WOW64) AppleWebKit/537.4 (KHTML, like Gecko) Chrome/22.0.1229.79 Safari/537.4" 
                   accept="*/*" 
                   acceptCharset="ISO-8859-1,utf-8;q=0.7,*;q=0.3" 
                   acceptLanguage="en-US,en;q=0.8"
      />
  </browserConfigGroup>
</configuration>